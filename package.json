{
  "name": "river-bathymetry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "gh-pages -d dist",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "gh-pages": "^2.0.1",
    "mapbox-gl": "^0.50.0",
    "vue": "^2.5.21",
    "vue2mapbox-gl": "^0.4.50",
    "vuetify": "^1.3.15",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.2",
    "@storybook/addon-info": "^4.1.2",
    "@storybook/addon-links": "^4.1.2",
    "@storybook/addon-storyshots": "^4.1.6",
    "@storybook/vue": "^4.1.6",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "jest": "^23.6.0",
    "jest-vue-preprocessor": "^1.4.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "storybook-addon-specifications": "^2.1.2",
    "storybook-chromatic": "^1.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-jest": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@storybook/.*\\.vue$))"
    ],
    "moduleFileExtensions": [
      "vue",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
